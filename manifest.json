{
  "name": "Swiftype Sidebar Search",
  "author": {
    "name": "Craig Gulliver",
    "email": "craiggulliver@yahoo.com"
  },
  "defaultLocale": "en",
  "private": true,
  "location": ["ticket_sidebar","new_ticket_sidebar"],
  "version": "1.0",
  "frameworkVersion": "1.0",
  "parameters": [
    {
      "name": "engine_key",
      "type": "text",
      "secure": false,
      "required": true
    },
    {
      "name": "per_page",
      "type": "number",
      "secure": false,
      "required": true,
      "default": "10"
    },
    {
      "name": "title_property",
      "type": "text",
      "secure": false,
      "required": true,
      "default": "title"
    },
    {
      "name": "type_property",
      "type": "text",
      "secure": false
    },
    {
      "name": "enable_suggestions",
      "type": "checkbox",
      "secure": false,
      "default": true
    }
  ]
}
